{
  "name": "xotoboil-storybook-stencil",
  "version": "0.0.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/xotoboil-storybook-stencil/xotoboil-storybook-stencil.esm.js",
  "engines": {
    "node": "v16.20.1",
    "npm": ">= 9.8.0",
    "yarn": ">= 2.0.0"
  },
  "files": [
    "dist/",
    "loader/",
    "src/styles/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "stencil build --docs",
    "build:watch": "stencil build --watch",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006",
    "storybook:watch": "concurrently \"yarn storybook\" \"yarn build:watch\"",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@stencil/core": "^4.1.0",
    "@stencil/sass": "^3.0.5",
    "@storybook/web-components-webpack5": "7.4.0-alpha.2",
    "husky": "^8.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.22.11",
    "@babel/plugin-syntax-jsx": "^7.22.5",
    "@babel/plugin-transform-react-jsx": "^7.22.5",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@ljcl/storybook-addon-cssprops": "^3.1.0",
    "@storybook/addon-essentials": "7.4.0-alpha.2",
    "@storybook/addon-mdx-gfm": "7.4.0-alpha.2",
    "@storybook/html-webpack5": "7.4.0-alpha.2",
    "@storybook/manager-api": "7.4.0-alpha.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/theming": "7.4.0-alpha.2",
    "@types/jest": "^29.5.4",
    "@types/node": "^20.5.6",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.1",
    "eslint": "^8.48.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "jest": "^29.6.4",
    "jest-cli": "^29.6.4",
    "jsx-dom": "^8.0.7",
    "prettier": "^3.0.2",
    "puppeteer": "^21.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass-loader": "^13.3.2",
    "storybook": "^7.4.0-alpha.2",
    "typescript": "^5.2.2"
  },
  "license": "MIT",
  "packageManager": "yarn@3.6.2"
}

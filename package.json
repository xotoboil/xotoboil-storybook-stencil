{
  "name": "xotoboil-storybook-stencil",
  "version": "0.0.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/xotoboil-storybook-stencil/xotoboil-storybook-stencil.esm.js",
  "engines": {
    "node": "v16.20.1",
    "npm": ">= 9.8.0",
    "yarn": ">= 2.0.0"
  },
  "files": [
    "dist/",
    "loader/",
    "src/styles/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "build": "stencil build --docs",
    "build:watch": "stencil build --watch",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test:watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "storybook": "storybook dev -p 6006",
    "storybook:watch": "concurrently \"yarn storybook\" \"yarn build:watch\"",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@stencil/core": "^2.13.0",
    "@stencil/sass": "^3.0.5",
    "husky": "^8.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-react": "^7.22.5",
    "@ljcl/storybook-addon-cssprops": "^3.1.0",
    "@storybook/addon-essentials": "^7.3.1",
    "@storybook/addon-interactions": "^7.3.1",
    "@storybook/addon-links": "^7.3.1",
    "@storybook/html": "^7.3.1",
    "@storybook/html-webpack5": "^7.3.1",
    "@storybook/manager-api": "^7.3.2",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/theming": "^7.3.2",
    "@types/jest": "^27.0.3",
    "@types/node": "^20.5.1",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "babel-loader": "^8.2.3",
    "concurrently": "^8.2.0",
    "eslint": "^8.47.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "jest": "^27.4.5",
    "jest-cli": "^27.4.5",
    "prettier": "^3.0.2",
    "puppeteer": "^13.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass-loader": "^13.3.2",
    "storybook": "^7.3.1",
    "typescript": "^5.1.6"
  },
  "license": "MIT",
  "packageManager": "yarn@3.6.2"
}
